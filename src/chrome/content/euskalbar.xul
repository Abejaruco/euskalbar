          <menuitem id="euskalbar-language-eu_en" label="EU ‣ EN" tooltiptext="&euskalbar.language.eu_en;" oncommand="euskalbar.setEuskalbarLang('eu', 'en'); euskalbar.setEuskalbarDictionaries('en'); event.stopPropagation();"/>
          <menuitem id="euskalbar-language-en_eu" label="EN ‣ EU" tooltiptext="&euskalbar.language.en_eu;" oncommand="euskalbar.setEuskalbarLang('en', 'eu'); euskalbar.setEuskalbarDictionaries('en'); event.stopPropagation();"/>
          <menuseparator/>
          <menuitem id="euskalbar-language-la_eu" label="LA ‣ EU" tooltiptext="&euskalbar.language.la_eu;" oncommand="euskalbar.setEuskalbarLang('la', 'eu'); euskalbar.setEuskalbarDictionaries('la'); event.stopPropagation();"/>
          <menuseparator/>
          <menuitem id="euskalbar-language-eu_jp" label="EU ‣ JA" tooltiptext="&euskalbar.language.eu_jp;" oncommand="euskalbar.setEuskalbarLang('eu', 'jp'); euskalbar.setEuskalbarDictionaries('jp'); event.stopPropagation();"/>
        </menupopup>
      </toolbarbutton>
    </toolbaritem>
      <hbox>
