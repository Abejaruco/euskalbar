          <menuitem id="euskalbar-language-eu_fr" label="EU ‣ FR" tooltiptext="&euskalbar.language.eu_fr;" oncommand="euskalbar.setEuskalbarLang('eu', 'fr'); euskalbar.setEuskalbarDictionaries('fr'); event.stopPropagation();"/>
